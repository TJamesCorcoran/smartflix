<h1>Editing email_campaign</h1>

<%= error_messages_for :email_campaign %>

<%= form_for(:email_campaign, :url => email_campaign_path(@email_campaign), :html => { :method => :put }) do |f| %>
  <%= render :partial => 'form', :object => f, :locals => { :reduced_frequency => @reduced_frequency } %>
  
  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Back', email_campaigns_path %>
